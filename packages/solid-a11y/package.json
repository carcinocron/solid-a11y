{
  "name": "solid-a11y",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A collection of accessibility components and helpers for SolidJS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dairyisscary/solid-a11y.git"
  },
  "keywords": [
    "a11y",
    "accessibility",
    "components",
    "solid",
    "solidjs"
  ],
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/solid-a11y.es.js",
        "require": "./dist/solid-a11y.umd.js"
      }
    },
    "main": "dist/solid-a11y.umd.js",
    "module": "dist/solid-a11y.es.js",
    "types": "types/index.d.ts"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.publish.json && vite build",
    "clean": "rm -rf types dist",
    "test": "jest"
  },
  "main": "src/index.tsx",
  "peerDependencies": {
    "solid-js": "^1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/user-event": "^14.0.0-beta.11",
    "@types/jest": "^27.4.1",
    "babel-jest": "^27.5.1",
    "babel-preset-solid": "^1.3.13",
    "enhanced-resolve-jest": "^1.1.0",
    "jest": "^27.5.1",
    "jest-node-exports-resolver": "^1.1.5",
    "solid-js": "^1.3.13",
    "solid-testing-library": "^0.3.0"
  }
}
